{
  "name": "nextjs-starter",
  "version": "0.0.0",
  "license": "MIT",
  "author": "Nelson Lai <me@nelsonlai.dev> (https://github.com/nelsonlaidev/)",
  "type": "module",
  "scripts": {
    "build": "next build",
    "check": "pnpm lint && pnpm typecheck && pnpm format:check && pnpm knip",
    "clean": "rm -rf .next",
    "dev": "next dev",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "preinstall": "npx --yes only-allow pnpm",
    "knip": "knip",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint --fix .",
    "prepare": "lefthook install",
    "start": "next start",
    "typecheck": "tsc --noEmit",
    "typegen": "next typegen"
  },
  "dependencies": {
    "next": "16.0.10",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@nelsonlaidev/eslint-config": "2.4.2",
    "@nelsonlaidev/prettier-config": "0.1.1",
    "@nelsonlaidev/typescript-config": "1.1.1",
    "@tailwindcss/postcss": "4.1.18",
    "@types/node": "24.10.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "eslint": "9.39.2",
    "knip": "5.73.4",
    "lefthook": "2.0.11",
    "postcss-load-config": "6.0.1",
    "prettier": "3.7.4",
    "prettier-plugin-packagejson": "2.5.20",
    "tailwindcss": "4.1.18",
    "typescript": "5.9.3"
  },
  "packageManager": "pnpm@10.25.0",
  "engines": {
    "node": ">=24"
  }
}
